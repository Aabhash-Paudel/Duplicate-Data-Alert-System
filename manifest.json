{
    "manifest_version": 3,
    "name": "DDAS - Download Duplication Alert System",
    "version": "1.0",
    "permissions": ["downloads", "storage", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icons.png"
    }
  }
  